  "devDependencies": {
    "@types/leaflet": "^1.9.20",
    "concurrently": "^9.2.1"
  },
  "dependencies": {
    "leaflet": "^1.9.4",
    "mapbox-gl": "^3.15.0",
    "next": "^15.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-leaflet": "^4.2.1",
    "react-map-gl": "^8.0.4"
  }
}
