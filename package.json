{
  "name": "monastery360",
  "version": "1.0.0",
  "description": "Monorepo for Monastery360 (Next.js frontend + FastAPI backend)",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && .venv\\Scripts\\python.exe -m uvicorn main:app --reload --port 8000"
  },
